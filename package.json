{
  "name": "nooro-fullstack-take-home",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:app": "next dev",
    "dev:server": "nodemon --watch server --exec ts-node --esm server/index.ts",
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:server\"",
    "build": "tsc && cd app && next build",
    "start:app": "cd app && next start",
    "start:server": "node dist/index.js",
    "start": "concurrently \"npm run start:app\" \"npm run start:server\"",
    "prisma-seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "next": "15.5.0",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
